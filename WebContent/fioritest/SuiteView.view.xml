<core:View xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	controllerName="fioritest.SuiteView" xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Page showHeader="true" showNavButton="false">
	
	<customHeader>
		<Bar>
<!--  		<contentLeft>
		<Text text = "Left text"></Text>
		</contentLeft>  
		
		<contentMiddle>
		<Text text = "Ghost Inspector"></Text>
		</contentMiddle>  -->
		
		<contentRight>
		<Button text="Suite" type="Accept" press="onSuitePress"></Button>
		</contentRight>		
		</Bar>
		</customHeader>
	
		<content>

			<Table id="idProducts" inset="false" items="{path: 'TMODEL>/'}" mode="MultiSelect">
				<headerToolbar>
					<Toolbar>
						<Title text="Batch Details" level="H2" />
					</Toolbar>
				</headerToolbar>
				<columns>

					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
						<Text text="Course"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
						<Text text="Batch"/>
					</Column>
					
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text="{TMODEL>value}" />
							<Text text="{TMODEL>name}" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>





		</content>
	</Page>
</core:View>